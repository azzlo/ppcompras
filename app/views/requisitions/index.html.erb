<p id="notice"><%= notice %></p>

<h1>Requisitions</h1>

<table>
  <thead>
    <tr>
      <th>Number</th>
      <th>Create at</th>
      <th>Item</th>
      <th>Material</th>
      <th>Description</th>
      <th>Quantity</th>
      <th>Unit</th>
      <th>Contact person</th>
      <th>Observations</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @requisitions.each do |requisition| %>
      <tr>
        <td><%= requisition.number %></td>
        <td><%= requisition.create_at %></td>
        <td><%= requisition.item %></td>
        <td><%= requisition.material %></td>
        <td><%= requisition.description %></td>
        <td><%= requisition.quantity %></td>
        <td><%= requisition.unit %></td>
        <td><%= requisition.contact_person %></td>
        <td><%= requisition.observations %></td>
        <td><%= link_to 'Show', requisition %></td>
        <td><%= link_to 'Edit', edit_requisition_path(requisition) %></td>
        <td><%= link_to 'Destroy', requisition, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Requisition', new_requisition_path %>
